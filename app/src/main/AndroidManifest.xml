<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.guanyin.sardar.pha">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name_shorter"
        android:supportsRtl="true"
        android:theme="@style/NoActionBarTheme">
        <activity android:name="com.guanyin.sardar.pha.FunctionActivity">
        </activity>
        <activity
            android:name="com.guanyin.sardar.pha.EntranceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.guanyin.sardar.pha.alert.ClockEditActivity">
        </activity>
        <activity
            android:name="com.guanyin.sardar.pha.test_other_function.SensorActivity">
        </activity>

        <receiver
            android:name=".alert.receiver.AlarmReceiver"
            android:process=":remote"/>
        <service android:name=".alert.service.MusicService"/>
    </application>

</manifest>